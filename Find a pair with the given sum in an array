import java.util.*;
public class Main {
	public static void main(String[] args) {
		int a[]=new int[]{1,5,3,9,3};
		int sum=12,count=0;
		Arrays.sort(a);
		int i=0,j=a.length-1;
		while(i<j)
		{
			if(a[i]+a[j]==sum)
			{
				System.out.println("pair :"+a[i]+" "+a[j]);
				count++;
				i++;
			}
			else if(a[i]+a[j]<sum)
			i++;
			else
			j--;
		}
		System.out.println(count);
		
	}
}
